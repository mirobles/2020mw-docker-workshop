export IP=$(ifconfig en0 | grep inet | awk '$1=="inet" {print $2}')

#Aliases
alias drunempty='docker run --rm -it --privileged -v "/var/run/docker.sock:/var/run/docker.sock" -v "/usr/local/bin/docker:/usr/local/bin/docker" -w /usr/src/app'
alias drun='docker run --rm -it --privileged -v "/var/run/docker.sock:/var/run/docker.sock" -v "/usr/local/bin/docker:/usr/local/bin/docker" -v "${PWD}:/usr/src/app" -w /usr/src/app'
alias noded='drun -v /usr/src/app/node_modules node:12.14.0 bash'
alias python3d='drun python:3 bash'
alias pythond='drun python:2.7 bash'
alias ubuntud='drun ubuntu:latest bash'
alias god='drun golang:latest bash'
alias gcloudd='drun -v "gcloud:/root/.config" google/cloud-sdk:latest bash'
alias javad='drun java:latest bash'
alias up='docker-compose up'
alias down='docker-compose down'
alias build='docker-compose build'
alias pull='docker-compose pull'
alias dc='docker-compose'
alias drm='docker rm -f'
alias dls='docker ps -a'
alias dcrun='docker-compose run --rm'
alias dexec='docker exec -it'
alias dbash='dc exec'
alias drm_all='docker rm -f $(docker ps -aq)'
alias clean='docker system prune -f && docker volume prune -f && docker image prune -f'
